    "sass-loader": "^8.0.2",
    "uuid": "^8.1.0",
    "velocity-animate": "^1.5.2",
    "velocityjs": "^2.0.0",
    "vue": "^2.6.11",
    "vue-loader": "latest",
    "vue-router": "^3.2.0",
